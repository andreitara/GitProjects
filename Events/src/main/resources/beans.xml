<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <bean id="andrei" class="Person">
              <property name="firstName" value="Andrei"/>
              <property name="lastName" value="Tara"/>
              <property name="age" value="23"/>
       </bean>

       <bean id="start" class="events.StartEvent"/>
       <bean id="stop"  class="events.StopEvent"/>
       <bean id="initiate" class="events.InitiateEvent"/>
       <bean id="close" class="events.CloseEvent"/>

</beans>